# üîê OKX Trading Bot Configuration
# Copy this file to .env and fill in your credentials
# DO NOT share this file or commit it to git

# ============================================================================
# OKX API Credentials
# ============================================================================
OKX_API_KEY=your_api_key_here
OKX_API_SECRET=your_api_secret_here
OKX_PASSPHRASE=your_passphrase_here

# ============================================================================
# Telegram Bot Configuration
# ============================================================================
TELEGRAM_BOT_TOKEN=your_bot_token_here
TELEGRAM_CHAT_ID=your_chat_id_here

# ============================================================================
# Optional: Advanced Settings
# ============================================================================
# Uncomment and modify if you want to customize behavior

# Target Profit Percentages
# TARGET_PROFIT_MIN=2.0
# TARGET_PROFIT_MAX=7.0

# Trading Timeframes
# TREND_TIMEFRAME=4h
# ENTRY_TIMEFRAME=15m

# Analysis Interval (seconds)
# CHECK_INTERVAL=300

# Risk Level (CONSERVATIVE, MODERATE, AGGRESSIVE)
# RISK_LEVEL=MODERATE

# Maximum Coins to Analyze
# MAX_COINS=25

# Enable Caching
# ENABLE_CACHE=true
# CACHE_TIMEOUT=300

# Logging Level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
# LOG_LEVEL=INFO
